# JARVIS Environment Variables Template
# Copy this to .env and fill in your values

# Environment (development, production, testing)
JARVIS_ENV=development

# LLM Configuration
LLM_API_KEY=your_llm_api_key_here
LLM_PROVIDER=qwen
LLM_MODEL=qwen2.5-coder:7b

# Temporal Configuration (for production)
TEMPORAL_ADDRESS=localhost:7233

# Database Configuration (for production)
DATABASE_URL=postgresql://user:password@localhost:5432/jarvis

# Email Configuration
EMAIL_USERNAME=your_email@gmail.com
EMAIL_PASSWORD=your_app_password

# SMTP Configuration
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587

# Voice Configuration
TTS_ENABLED=true
STT_ENABLED=true

# Optional: Override any config value
# MONITORING_ENABLED=true
# SECURITY_SANDBOX_ENABLED=true
